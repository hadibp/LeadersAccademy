<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
    <link rel="stylesheet" href="../public/stylesheets/style.css" />


    <title>Profile</title>

  </head>
  <style>
      .hi{
          height: 100;
      }
  </style>
  <body>
      <nav>
        <%- include('../partials/navbar'); %>

    </nav>
      <div class="row hi">
    <div class="col-4  bg-light">

        <h1>sidebar</h1>
    </div>

<div class="col-8 bg-success">
    <h1 class="porfile">
        Profile
    </h1>
    <p>name: <$ users.name></p>
    <p>age: <$ users.age></p>
    <p>email: <$ users.email></p>
    <p>mobile: <$ users.mobile></p>

</div>
<% for(var i=0; i<register.length; i++){ %>
    <div class="card m-3 rounded" style="width: 18rem">
      <img src="../public/images/profileimages/<%= register[i].img %>" class="rounded-circle image mx-auto d-block mt-4" alt="profile" />
      <div class="card-body">
        <h5 class="card-title " style="text-align: center;"><%= register[i].studname %></h5>
        <p class="card-text " style="text-align: center;">
          <%= register[i].program %>
        </p>
      </div>
    </div>
    <% } %>

</div>
    <!-- Option 1: jQuery and Bootstrap Bundle (includes Popper) -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-fQybjgWLrvvRgtW6bFlB7jaZrFsaBXjsOMm/tB9LTS58ONXgqbR9W8oWht/amnpF" crossorigin="anonymous"></script>

    
  </body>
</html>